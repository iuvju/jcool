{
  "name" : "Jcool",
  "version" : "1.0.0",
  "description" : "Jcool",
   "permissions": [
    "tabs",
    "storage"
  ],
 "browser_action": {
    "default_title": "Jcool!",
	"default_icon": "img/16_16_off.png",
    "default_popup": "popup.html"
  },
  "options_page":"option.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
